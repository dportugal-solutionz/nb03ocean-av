      <Name>Room Combine</Name>
      <Row>
        <Name>Row</Name>
        <Widget>
          <WidgetId>combine-none-btn</WidgetId>
          <Name>Separate All Rooms</Name>
          <Type>Button</Type>
          <Options>size=2</Options>
        </Widget>
        <Widget>
          <WidgetId>combine-all-btn</WidgetId>
          <Name>Combine All Rooms</Name>
          <Type>Button</Type>
          <Options>size=2</Options>
        </Widget>
      </Row>
      <PageId>roomcombine-pg</PageId>
      <Options>hideRowNames=1</Options>
    </Page>
  </Panel>
</Extensions>
