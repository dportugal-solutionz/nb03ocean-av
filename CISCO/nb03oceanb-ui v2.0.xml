    <Page>
      <Name>Displays</Name>
      <Row>
        <Name>Main Displays</Name>
        <Widget>
          <WidgetId>main-display-power-on</WidgetId>
          <Name>Power On</Name>
          <Type>Button</Type>
          <Options>size=2</Options>
        </Widget>
        <Widget>
          <WidgetId>main-display-power-off</WidgetId>
          <Name>Power Off</Name>
          <Type>Button</Type>
          <Options>size=2</Options>
        </Widget>
      </Row>
      <PageId>displays-pg</PageId>
      <Options/>
    </Page>
    <Page>
      <Name>Microphones</Name>
      <Row>
        <Name>Lavalier 1</Name>
        <Widget>
          <WidgetId>Mic1Level</WidgetId>
          <Type>Slider</Type>
          <Options>size=3</Options>
        </Widget>
        <Widget>
          <WidgetId>Mic1Mute</WidgetId>
          <Name>Mute</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Lavalier 2</Name>
        <Widget>
          <WidgetId>Mic2Level</WidgetId>
          <Type>Slider</Type>
          <Options>size=3</Options>
        </Widget>
        <Widget>
          <WidgetId>Mic2Mute</WidgetId>
          <Name>Mute</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Ceiling Mic</Name>
        <Widget>
          <WidgetId>CeilingMicMute</WidgetId>
