<Extensions>
  <Version>1.11</Version>
  <Panel>
    <Order>1</Order>
    <PanelId>roomcontrols</PanelId>
    <Origin>local</Origin>
    <Location>HomeScreenAndCallControls</Location>
    <Icon>Lightbulb</Icon>
    <Name>Room Controls</Name>
    <ActivityType>Custom</ActivityType>
    <Page>
      <Name>Lighting</Name>
      <Row>
        <Name>Lights</Name>
        <Widget>
          <WidgetId>lightzone01-on-btn</WidgetId>
          <Name>On</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>lightzone01-75-btn</WidgetId>
          <Name>75</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>lightzone01-50-btn</WidgetId>
          <Name>50</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>lightzone01-25-btn</WidgetId>
          <Name>25</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>lightzone01-off-btn</WidgetId>
          <Name>Off</Name>
          <Type>Button</Type>
          <Options>size=4</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Blackout &amp; Solar Shades</Name>
        <Widget>
          <WidgetId>shadezone01-open-btn</WidgetId>
          <Name>Open</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>shadezone01-close-btn</WidgetId>
          <Name>Close</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>shadezone01-up-btn</WidgetId>
          <Type>Button</Type>
          <Options>size=1;icon=arrow_up</Options>
        </Widget>
        <Widget>
          <WidgetId>shadezone01-down-btn</WidgetId>
          <Type>Button</Type>
          <Options>size=1;icon=arrow_down</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Solar Shades</Name>
        <Widget>
          <WidgetId>shadezone02-open-btn</WidgetId>
          <Name>Open</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>shadezone02-close-btn</WidgetId>
          <Name>Close</Name>
          <Type>Button</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>shadezone02-up-btn</WidgetId>
          <Type>Button</Type>
          <Options>size=1;icon=arrow_up</Options>
        </Widget>
        <Widget>
          <WidgetId>shadezone02-down-btn</WidgetId>
          <Type>Button</Type>
          <Options>size=1;icon=arrow_down</Options>
        </Widget>
      </Row>
      <PageId>lighting-pg</PageId>
      <Options/>
